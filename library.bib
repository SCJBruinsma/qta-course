@Article{Jahn2014a,
  author   = {Jahn, Detlef},
  title    = {What is left and right in comparative politics? A response to Simon Franzmann},
  doi      = {10.1177/1354068813520273},
  eprint   = {http://dx.doi.org/10.1177/1354068813520273},
  number   = {2},
  pages    = {297--301},
  volume   = {20},
  abstract = {Recently, I developed a left--right index in Party Politics which provoked some reaction. In particular, my claim that I use a deductive approach was questioned, it being stressed that other indices are also constructed deductively and that, in turn, my index is not adequately deductive at all. In this response to Simon Franzmanns (2013) comment I stress the advantages and pitfalls of both inductive and deductive approaches. Additionally, I elaborate why I perceive most existing left--right indices as primarily inductive in nature and stress the importance of theory in a deductive approach. I question that a high correlation with other left--right indices is a quality criterion of construct validity. I then correct some misinterpretations fostered by Franzmann and summarize the logic of my index. Finally, I give two brief examples of the merit and analytical potential of a deductive left--right score.},
