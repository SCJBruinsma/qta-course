@Article{Gabel2002a,
  author   = {Gabel, Matthew and Hix, Simon},
  title    = {Defining the Eu Political Space: An Empirical Study of the European Elections Manifestos, 1979-1999},
  doi      = {10.1177/001041402236309},
  eprint   = {https://doi.org/10.1177/001041402236309},
  number   = {8},
  pages    = {934--964},
  volume   = {35},
  abstract = {Descriptive models of the EU political space are fundamental components of both descriptions of EU politics and theories of EU policy making. But there is no consensus on which, if any, model accurately captures the EU political space. In this article, the authors seekto advance their understanding of the EU political space by bringing evidence to bear on the accuracy of four extant models. Following the methodology used to analyze the national political space of Euro-pean democracies, they assemble and code manifestos of party federations written prior to Euro-pean elections. They then use confirmatory factor analysis to analyze the underlying structure of these party federations'policy positions. They also examine temporal differences in party feder-ation positions over several sets of issues. The results of the study indicate that the EU political space is basically one dimensional and similar to the domestic Left-Right dimension in policy content.},
  journal  = {Comparative Political Studies},
  year     = {2002},
}

