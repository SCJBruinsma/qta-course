  title     = {Intra-Party Preference Heterogeneity and Faction Membership in the 15áµÊ° German Bundestag: A Computational Text Analysis of Parliamentary Speeches},
  doi       = {10.1080/09644000903055823},
  issn      = {0964-4008},
