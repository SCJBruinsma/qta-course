@Article{Heinsohn2016a,
  author      = {Heinsohn, Till and Israel, Jonas and Marschall, Stefan and Schultze, Martin},
  title       = {Online-Wahlhilfen in Wahlk{\"{a}}mpfen},
  doi         = {10.1007/s41358-016-0055-y},
  issn        = {1430-6387},
  language    = {German},
  number      = {3},
  pages       = {253--277},
  volume      = {26},
  abstract    = {In this paper, we address the question whether and how VAAs have an effect on voters political interest, knowledge and participation. For our analysis, we draw on data generated by a three-wave panel study roughly representative for the German online-population which was exclusively designed to study effects of the Wahl-O-Mat during the 2014 European Elections campaign. By analyzing the intra-individual change of Wahl-O-Mat users and non-users concerning their interest, knowledge and participation, we are able to track the mobilization potential of election campaigning and whether the VAA use has reinforcing effects on this potential. Empirical findings of the three-wave panel study indicate that indeed during the campaign an increase in campaign interest can be found and a slight increase in political knowledge and the willingness to participate in the elections.},
  affiliation = {Institut f{\"{u}}r Sozialwissenschaften, Philosophische Fakult{\"{a}}t, Heinrich-Heine-Universit{\"{a}}t D{\"{u}}sseldorf},
